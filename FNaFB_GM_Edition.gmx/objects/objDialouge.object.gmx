  keyboard_key_release(vk_enter);
  audio_play_sound(sndpop,90,0);
  if !printed {
      dtext=text;
      printed=1;
  } else {
    if yn {
      global.input=option;
    } else {
      global.input=0;
    }
    inout=1;
  }
}
if yn{//Up and down
  if keyboard_check_pressed(vk_up){
    option--;
    if option&lt;0 option=options-1;
