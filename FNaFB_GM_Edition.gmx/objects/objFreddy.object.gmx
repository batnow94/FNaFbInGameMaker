            <string>///Player Controls
if keyboard_check(vk_shift) {
    mspeed=dspeed*2;
} else {
    mspeed=dspeed;
}
if instance_position(x,y,objStairs) {
    if instance_nearest(x,y,objStairs).ud {
        if udlri==1 {
            onstage=0;
        } else {
            onstage=1;
        }
    } else {
        if udlri==1 {
            onstage=1;
        } else {
            onstage=0;
        }
    }
}
if talking {
    udlr=4;
}
if (move or udlri==4) &amp;&amp; !talking {
    udlr=4;
    if keyboard_check(vk_up) {
        if place_free(x,y-32) {
            if onstage &amp;&amp; !instance_position(x,y-32,objWallLevel1) {
                udlri=0;
                move=0;
            } else {
                udlri=0;
                move=0;
            }
        }
    }
    if keyboard_check(vk_down) {
        if place_free(x,y+32) {
            if onstage &amp;&amp; !instance_position(x,y+32,objWallLevel1) {
                udlri=1;
                move=0;
            } else {
                udlri=1;
                move=0;
            }
        }
    }
    if keyboard_check(vk_left) {
        if place_free(x-32,y) {
            if onstage &amp;&amp; !instance_position(x-32,y,objWallLevel1) {
                udlri=2;
                move=0;
            } else {
                udlri=2;
                move=0;
            }
        }
    }
    if keyboard_check(vk_right) {
        if place_free(x+32,y) {
            if onstage &amp;&amp; !instance_position(x+32,y,objWallLevel1) {
                udlri=3;
                move=0;
            } else {
                udlri=3;
                move=0;
            }
        }
    }
} else {
    switch udlri {
        case 0:
            y-=mspeed;
        break;
        case 1:
            y+=mspeed;
        break;
        case 2:
            x-=mspeed;
        break;
        case 3:
            x+=mspeed;
        break;
    }
    if place_snapped(32,32) {
        move=1;
    }
}

animatePlayer(udlri,mspeed);
